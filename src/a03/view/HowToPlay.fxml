<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.111"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="a03.view.HowToPlayController">
	<children>
		<ImageView fx:id="_imageView" fitHeight="500.0" fitWidth="700.0"
			layoutX="1.0" layoutY="101.0" pickOnBounds="true" preserveRatio="true"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
		<BorderPane layoutX="10.0" layoutY="10.0" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="450.0" prefWidth="700.0" AnchorPane.bottomAnchor="0.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
			AnchorPane.topAnchor="0.0">
			<bottom>
				<ButtonBar prefHeight="40.0" prefWidth="200.0"
					BorderPane.alignment="CENTER">
					<opaqueInsets>
						<Insets />
					</opaqueInsets>
					<BorderPane.margin>
						<Insets right="30.0" />
					</BorderPane.margin>
					<padding>
						<Insets bottom="20.0" />
					</padding>
					<buttons>
						<JFXButton fx:id="_previous" />
						<JFXButton fx:id="_next" />
					</buttons>
				</ButtonBar>
			</bottom>
			<top>
				<JFXButton fx:id="_back" onAction="#handleBack"
					BorderPane.alignment="TOP_LEFT">
					<BorderPane.margin>
						<Insets left="10.0" top="10.0" />
					</BorderPane.margin>
				</JFXButton>
			</top>
		</BorderPane>
	</children>
</AnchorPane>
